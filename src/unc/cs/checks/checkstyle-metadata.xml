<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
	<rule-group-metadata name="UNC custom checks"
		priority="1600">
		<rule-metadata name="%ClassDefined.name" internal-name="ClassDefined"
			parent="unc.tools.checkstyle.AnExtendibleTreeWalker">
			<alternative-name internal-name="unc.cs.checks.ClassDefinedCheck" />
			<description>%ClassDefined.desc</description>			
		</rule-metadata>
		<rule-metadata name="%InterfaceDefined.name" internal-name="InterfaceDefined"
			parent="unc.tools.checkstyle.AnExtendibleTreeWalker">
			<alternative-name internal-name="unc.cs.checks.InterfaceDefinedCheck" />
			<description>%InterfaceDefined.desc</description>			
		</rule-metadata>
			<rule-metadata name="%STBuilder.name" internal-name="STBuilder"
			parent="unc.tools.checkstyle.AnExtendibleTreeWalker">
			<alternative-name internal-name="unc.cs.checks.STBuilderCheck" />
			<description>%STBuilder.desc</description>			
		</rule-metadata>
		<rule-metadata name="%NoDuplicateShortTypeName.name" internal-name="NoDuplicateShortTypeName"
			parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.NoDuplicateShortTypeNameCheck" />
			<description>%NoDuplicateShortTypeNameDefined.desc</description>			
		</rule-metadata>
		<rule-metadata name="%MethodLimit.name" internal-name="MethodLimit"
			parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.MethodLimitCheck" />
			<description>%MethodLimit.desc</description>
			<property-metadata name="max" datatype="Integer"
				default-value="30">
				<description>The maximum number of allowed methods.</description>
			</property-metadata>
			<quickfix classname="unc.cs.checks.MethodLimitQuickfix" />
		</rule-metadata>
		<rule-metadata name="%VariableHasClassType.name"
			internal-name="VariableHasClassType" parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.VariableHasClassTypeCheck" />
			<description>%VariableHasClassType.desc</description>
		</rule-metadata>
		<rule-metadata name="%ClassHasOneInterface.name"
			internal-name="ClassHasOneInterface" parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.ClassHasOneInterfaceCheck" />
			<description>%ClassHasOneInterface.desc</description>
		</rule-metadata>
		<rule-metadata name="%ClassHasAtLeastOneInterface.name"
			internal-name="ClassHasAtLeastOneInterface" parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.ClassHasAtLeastOneInterfaceCheck" />
			<description>%ClassHasAtLeastOneInterfaceCheck.desc</description>
		</rule-metadata>		
		<rule-metadata name="%IllegalMethodCall.name"
			internal-name="IllegalMethodCall" parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.IllegalMethodCallCheck" />
			<description>%MethodCall.desc</description>
		</rule-metadata>
		<rule-metadata name="%IllegalTypeImported.name"
			internal-name="IllegalTypeImported" parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.IllegalTypeImportedCheck" />
			<description>%IllegalTypeImported.desc</description>
		</rule-metadata>
		<rule-metadata name="%IllegalInitCall.name"
			internal-name="IllegalInitCall" parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.IllegalInitCallCheck" />
			<description>%IllegalInitCall.desc</description>
		</rule-metadata>
		<rule-metadata name="%IllegalComponentInstantiation.name"
			internal-name="IllegalComponentInstantiation" parent="TreeWalker">
			<alternative-name internal-name="unc.cs.checks.IllegalComponentInstantiationCheck" />
			<description>%IllegalComponentInstantiation.desc</description>
		</rule-metadata>
	</rule-group-metadata>
</checkstyle-metadata>
